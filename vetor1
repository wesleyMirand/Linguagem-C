#include <stdio.h>
#include <stdlib.h>

#define TAM 5

int main()
{
    //tipo nomeVar[tam];
    float vetNotas[TAM], media = 0; //valor da media tenque zero(nada) pq vai somar os valores dentro dela e dividi pela quantidade de valores
    vetNotas[0] = 5.9;//primeira posição dice zero valor 1
    vetNotas[1] = 6;
    vetNotas[2] = 7;
    vetNotas[3] = 9.3;
    vetNotas[4] = 8.4;

    media = (vetNotas[0] + vetNotas[1] +vetNotas[2]+vetNotas[3]+vetNotas[4]) / 5;

    printf("%.2f", media);

    return 0;
}
